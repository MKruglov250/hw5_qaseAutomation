<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sequential Multiple Browsers" parallel="false">
    <test name="Chrome Full">
        <parameter name="BrowserType" value="Chrome"/>
        <classes>
            <class name="org.example.LoginTest"/>
            <class name="org.example.TestCasePageTest"/>
            <class name="org.example.TestPlanPageTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Edge Full">
        <parameter name="BrowserType" value="Edge"/>
        <classes>
            <class name="org.example.LoginTest"/>
            <class name="org.example.TestCasePageTest"/>
            <class name="org.example.TestPlanPageTest"/>
        </classes>
    </test> <!-- Test -->
</suite>